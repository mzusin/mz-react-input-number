    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "esbuild": "^0.17.17",
    "mz-math": "^3.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
